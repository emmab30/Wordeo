  removeURIScheme: function removeURIScheme(uri, iterations) {
    iterations = iterations || 1;
    var result = uri;
    for (var i = 0; i < iterations; i++) {
      result = String(result).replace(/^[^\:]+\:\/\//, '');
    }
    return String(result);
  },

