{
  "name": "Configuration",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
        "type": "string",
        "required": true
    },
    "value": {
        "type": "string",
        "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [{
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "getLastVersion"
      ]
    }],
  "methods": {
    "userStatistics": {
        "returns": {
            "root": true,
            "type": "object"
        },
        "http": {
            "verb": "get",
            "path": "/user_statistics"
        }
    },
    "getLastVersion": {
        "accepts": {
            "arg": "data",
            "type": "object",
            "required": true,
            "http": {
                "source": "body"
            }
        },
        "returns": {
            "root": true,
            "type": "object"
        },
        "http": {
            "verb": "post",
            "path": "/last_version"
        }
    }
  }
}
